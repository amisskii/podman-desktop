summary: Execute smoke Playwright end-to-end tests. 
tag:
  - smoke
duration: 2h
framework: shell
test: |
  set +e
  echo $TMT_TEST_DATA
  cd $TMT_TREE
  pnpm install
  pnpm test:e2e:smoke
  echo $?
  EXIT_CODE=$?
  bash "$TMT_TREE/tests/tmt/scripts/create-results.sh" "$EXIT_CODE" smoke
result: custom
